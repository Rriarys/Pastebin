{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=PastebinDB;Trusted_Connection=True;TrustServerCertificate=True;" // локальный мс скл сервак
  },
  "AzureStorage": {
    "ConnectionString": "UseDevelopmentStorage=true"
  },
  "Redis": {
    "Connection": "localhost:6379" // 6379 — стандартный порт Redis
  },
  "Jwt": {
    "Key": "YourSuperSecureKeyThatIsAtLeast32CharactersLong",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience"
  },
  "HashService": {
    "Salt": "YourSaltValueHere", // Значение соли для хеширования
    "MinHashLength": "8" // Минимальная длина хеша
  }
}

